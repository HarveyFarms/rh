alias hcourse find-file-noselect course.org
alias course find-file course.org
alias hmain find-file-noselect main.cpp
alias main find-file main.cpp
alias log (find-file (expand-file-name (concat "~/rh/org/journal/" (format-time-string "%m-%d-%y") ".org")))
alias gpu pushd ~/rh ; m p; cd ~/cc ; m r ; ~ ; c r
alias gsy pushd ~/rh ; m git; ~/cc ; m git; ~ ; c r
alias m make $*
alias q $*
alias c clear $*
